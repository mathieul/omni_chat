<%= if @elm_module do %>
  <script>
    (function () {
      var node = document.getElementById('elm-main')
      var app = Elm.<%= @elm_module %>.embed(node)
      setTimeout(function () {
        <%= @elm_module %>(<%= raw if @elm_flags, do: Poison.encode!(@elm_flags), else: "{}" %>)
      }, 0)

      function Online(params) {
        app.ports.initApplication.send(params)
      }
    })()
  </script>
<% end %>
